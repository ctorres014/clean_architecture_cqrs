{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting": "Warning",
        "Microsoft.Hosting.LifeTime": "Warning"
      }
    },
    "Filter": [
      {
        "Args": {
          "Expression": "@mt = 'An unhandled exception has ocurred while executing'"
        },
        "Name": "ByExluding"
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "GloboTicketManagmentConnectionString": "server=(localdb)\\mssqlLocalDB;database=GloboTicketManagment;trusted_connection=true",
    "GloboTicketIdentityConnectionString": "server=(localdb)\\mssqlLocalDB;database=GloboTicketIdentity;trusted_connection=true"
  },
  "AllowedHosts": "*"
}
